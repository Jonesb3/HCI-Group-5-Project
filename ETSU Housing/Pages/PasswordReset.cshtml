@page
@model ETSU_Housing.Pages.PasswordResetModel
@{
    ViewData["Title"] = "Password Reset";
}

<div>
    <h2>Password Reset</h2>
    <form>
        <label>Email</label><br />
        <input type="email" id="email" name="email" /><br />
        <label>ETSU ID</label><br />
        <input type="number" id="enumber" name="enumber" /><br />
        <input type="checkbox" id="agreement" name="agreement" />
        <label>I accept the <a class="" asp-area="" asp-page="/Terms">Terms of Use</a> & <a class="" asp-area="" asp-page="/Privacy">Privacy Policy</a></label><br />
        <input type="submit" value="Reset Password" />
    </form><br />
</div>